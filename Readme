docker run --rm -it confluentinc/cp-kafka:latest bash

kafka-console-consumer --bootstrap-server kfakasda.eastus.cloudapp.azure.com:9092 --topic result_gora --from-beginning --timeout-ms 10000


kafka-topics --bootstrap-server kfakasda.eastus.cloudapp.azure.com:9092 --list


kafka-topics --bootstrap-server kfakasda.eastus.cloudapp.azure.com:9092 --describe --topic result_gora


